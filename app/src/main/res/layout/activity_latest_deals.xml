<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/back1"
    android:layout_height="match_parent"
    tools:context=".TopEats">


    <TableLayout

        android:layout_width="410dp"
        android:layout_height="561dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.421"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tabLayout"
        app:layout_constraintVertical_bias="0.0">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <SearchView
                android:id="@+id/searchView"
                android:layout_width="194dp"
                android:layout_height="30dp"
                android:layout_marginBottom="32dp"
                android:background="#E9EDEF"
                android:queryHint="Search" />

            <Button
                android:id="@+id/btnTopEats"
                android:layout_width="218dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:text="Back"
                app:layout_constraintVertical_bias="0.828" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="114dp">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="161dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/vegsan" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="215dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/chisa" />
        </TableRow>

        <TableRow
            android:layout_width="211dp"
            android:layout_height="159dp">

        </TableRow>

        <TableRow
            android:layout_width="271dp"
            android:layout_height="287dp">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="195dp"
                android:layout_height="133dp"
                app:srcCompat="@drawable/tusan" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="217dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/ches" />
        </TableRow>
    </TableLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="191dp"
        android:layout_height="195dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.004"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logos" />

    <Button
        android:id="@+id/LogOut"
        android:layout_width="129dp"
        android:layout_height="38dp"
        android:background="@drawable/customebtn"
        android:text="@string/LogOut"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.943"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072" />




</androidx.constraintlayout.widget.ConstraintLayout>