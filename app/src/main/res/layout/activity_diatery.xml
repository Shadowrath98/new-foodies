<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/gray"
    android:layout_height="match_parent"
    tools:context=".TopEats">


    <TableLayout

        android:layout_width="410dp"
        android:layout_height="561dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tabLayout"
        app:layout_constraintVertical_bias="0.0">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="2dp">

            <SearchView
                android:id="@+id/searchView"
                android:layout_width="194dp"
                android:layout_height="30dp"
                android:layout_marginBottom="32dp"
                android:background="#E9EDEF"
                android:queryHint="Search" />

            <Button
                android:id="@+id/btnTopEats"
                android:layout_width="197dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:text="Back"
                app:layout_constraintVertical_bias="0.828" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imagel3"
                android:layout_width="182dp"
                android:layout_height="218dp"
                app:srcCompat="@drawable/l1" />

            <ImageView
                android:id="@+id/imagel1"
                android:layout_width="209dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/l3" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imagel2"
                android:layout_width="208dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/l4" />

            <ImageView
                android:id="@+id/imagel4"
                android:layout_width="187dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/l2" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="143dp" />

    </TableLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="206dp"
        android:layout_height="195dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.005"
        app:srcCompat="@drawable/logos" />

    <Button
        android:id="@+id/LogOut"
        android:layout_width="129dp"
        android:layout_height="38dp"
        android:background="@drawable/customebtn"
        android:text="@string/LogOut"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.943"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072" />





</androidx.constraintlayout.widget.ConstraintLayout>